<odoo>
        <record id="cash_form" model="ir.ui.view">
            <field name="name">cash.cash.form</field>
            <field name="model">cash.cash</field>
            <field name="arch" type="xml">
                <form string="cash">
                    <header>
                <button name="button_confirm"
                            states="draft"
                            string="Confirm"
                            type="object"
                            class="oe_highlight"
                            groups="account.group_account_manager"
                    />
                <field name="state" widget="statusbar"
                           statusbar_visible="draft,posted"
                           statusbar_colors='{"draft":"red"}'/></header>
                    <sheet>
 <div class="oe_button_box" name="button_box">
                            <button class="oe_stat_button" name="button_journal_entries"
                                    string="Journal Entries" type="object"
                                    attrs="{'invisible':[('state','=','draft')]}" icon="fa-bars">
                                <field name="journal_entries" invisible="1"/></button>
                            <button class="oe_stat_button" name="button_account"
                                    string="Balance" type="object"
                                  attrs="{'invisible':[('cash_method','=','check')]}" icon="fa-book">
                                   <field name='balance' position="inside"/>
                            </button>
                    </div>
                        <group name="main">
                           <group name="left">
                                                           <field name="code" />
                                                           <field name="employee_id" />


                                                          <field name="date"/>


                                   <field name="payment_type" widget="radio" attrs="{'readonly': [('state', '=', 'posted')]}"/>

                                <field name="payment_partner" widget="radio" attrs="{'readonly': [('state', '=', 'posted')]}"/>
                              <field name="cash_method" widget="radio" />


                            </group>

                            <group name="right">
                            <field name="journal_id" attrs="{'invisible':[('cash_method', 'not in',('cash'))],'required': [('cash_method', 'in',('cash'))],'readonly': [('state', '=', 'posted')]}" options="{'no_create':True,'no_open':True}"/>
                            <field name="check_receive" attrs="{'required': [('payment_type','in',('receive_money')), ('cash_method','in',('check'))] ,'invisible': ['|',('payment_type','&lt;&gt;','receive_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}"  options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="check_send" attrs="{'required': [('payment_type','in',('send_money')), ('cash_method','in',('check'))] ,'invisible': ['|',('payment_type','&lt;&gt;','send_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}"  options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="check_safe" attrs="{'required': [('payment_type','in',('send_money','receive_money')), ('payment_partner','in',('partner','expense','other')),('cash_method', 'in',('check'))], 'invisible': ['|',('payment_type','&lt;&gt;',('send_money','receive_money')), ('payment_partner','&lt;&gt;','partner'),('cash_method', 'not in',('check'))],'readonly': [('state', '=', 'posted')],}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="amount" sum="Amount" attrs="{'invisible':[('payment_type', 'not in',('send_money'))],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="amount_re" attrs="{'invisible':[('payment_type', 'not in',('receive_money'))],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="check_number" attrs="{'required': [('payment_type','in',('send_money','receive_money')), ('payment_partner','in',('partner','expense','other')),('cash_method', 'in',('check'))], 'invisible': ['|',('payment_type','&lt;&gt;',('send_money','receive_money')), ('payment_partner','&lt;&gt;','partner'),('cash_method', 'not in',('check'))],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="due_date" attrs="{'required': [('payment_type','in',('send_money','receive_money')), ('payment_partner','in',('partner','other','expense')),('cash_method', 'in',('check'))], 'invisible': ['|',('payment_type','&lt;&gt;',('send_money','receive_money')), ('payment_partner','&lt;&gt;','partner'),('cash_method', 'not in',('check'))],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="bank" attrs="{'required': [('payment_type','in',('send_money')), ('cash_method','in',('check'))] ,'invisible': ['|',('payment_type','&lt;&gt;','send_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="bank_receive" attrs="{'invisible': ['|',('payment_type','&lt;&gt;','receive_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}" />
                            <field name="bank_account_receive" attrs="{'invisible': ['|',('payment_type','&lt;&gt;','receive_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}" />
                            <field name="bank_account" attrs="{'required': [('payment_type','in',('send_money')), ('cash_method','in',('check'))] ,'invisible': ['|',('payment_type','&lt;&gt;','send_money'), ('cash_method','&lt;&gt;','check')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="partner_type" attrs="{'required': [('payment_type','in',('send_money','receive_money')),('payment_partner','in',('partner'))] ,'invisible': [('payment_partner','not in',('partner'))],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="partner_id" attrs="{'required': [('payment_partner','=','partner')], 'invisible':[('payment_partner', '!=','partner')],'readonly': [('state', '=', 'posted')]}" context="{ 'default_supplier': payment_type == 'send_money', 'default_customer': payment_type == 'receive_money'}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="employee_expense" attrs="{'required': [('payment_partner','in',('custody','loan','expense'))],'invisible':[('payment_partner', 'not in',('expense','loan','custody'))],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="account_loan" attrs="{'invisible':[ ('payment_partner', '!=','loan')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True}" readonly="1"/>
                            <field name="account_other" attrs="{'required': [('payment_partner','=','other')], 'invisible': [('payment_partner','!=','other')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>
                            <field name="account_custody" attrs="{'required': [('payment_partner','in',('custody'))] ,'invisible':[ ('payment_partner', '!=','custody')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}" readonly="1"/>
                            <field name="partner_other" attrs="{'invisible': [('payment_partner','&lt;&gt;','other')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True}"/>
<!--                            <field name="account_id_expense" attrs="{'required': [('payment_partner','in',('expense'))], 'invisible':[('payment_partner', '!=','expense')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}"/>-->
                            <field name="analytic_account_id" attrs="{'required': [ ('payment_partner','=','expense')], 'invisible':[('payment_partner', '!=','expense')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}" />
                                                       <field name="catg_id" attrs="{'required': [ ('payment_partner','=','expense')], 'invisible':[('payment_partner', '!=','expense')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}" />

                            <field name="product_id" attrs="{'required': [ ('payment_partner','=','expense')], 'invisible':[('payment_partner', '!=','expense')],'readonly': [('state', '=', 'posted')]}" options="{'no_create_edit':True,'no_open':True,'no_create': True,}" />
                            <field name="payment_condation" attrs="{'required': [('payment_type','in','send_money'), ('payment_partner','in','loan')], 'invisible': ['|',('payment_type','&lt;&gt;','send_money'), ('payment_partner','&lt;&gt;','loan')],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="start_date_payment" attrs="{'required': [('payment_type','in','send_money'), ('payment_partner','in','loan')], 'invisible': ['|',('payment_type','&lt;&gt;','send_money'), ('payment_partner','&lt;&gt;','loan')],'readonly': [('state', '=', 'posted')]}"/>
                            <field name="description" attrs="{'readonly': [('state', '=', 'posted')]}"/>

                            </group>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                  <field name="message_follower_ids" widget="mail_followers"/>
                  <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>
     <record id="view_account_search" model="ir.ui.view">
