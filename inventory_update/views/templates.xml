<odoo>
    <data>
        <data noupdate="1">
    <record id="seq_item_request" model="ir.sequence">
        <field name="name">Picking</field>
        <field name="code">delivered.picking</field>
        <field name="prefix">WH/IN/%(y)s</field>
        <field name="padding">5</field>
    </record></data>
<!-- Inherit Form View to Modify it -->
<record id="stock_return_picking_inherit" model="ir.ui.view">
    <field name="name">stock.return.picking</field>
    <field name="model">stock.return.picking</field>
    <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
    <field name="arch" type="xml">

        <xpath expr="//tree//field[@name='product_id']" position="after">
                <field name="lot_id" readonly="1" force_save="1"/>
                <field name="max_quantity" readonly="1" force_save="1"/>
        </xpath>

    </field>
</record>
    </data>
</odoo>