<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

    <record model="ir.module.category" id="module_category_item_request">
        <field name="name">Item Request</field>
        <field name="description">Helps you request stock Items.</field>
        <field name="sequence">8</field>
    </record>

    <record id="item_request_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="category_id" ref="module_category_item_request"/>
    </record>
    <record id="item_request_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="implied_ids" eval="[(4, ref('item_request.item_request_user'))]"/>
        <field name="category_id" ref="module_category_item_request"/>
    </record>
</data>
</odoo>