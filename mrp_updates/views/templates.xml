<odoo>
    <data>
  <record id="batch_view_picking_form" model="ir.ui.view">
            <field name="name">stock.picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                     <field name="batch" attrs="{'invisible': [('batch', '=', False)]}"/>
                     <field name="mrp_product_id" attrs="{'invisible': [('mrp_product_id', '=', False)]}"/>
                </xpath>
            </field></record>
        <record id="batch_view_picking_tree" model="ir.ui.view">
            <field name="name">stock.picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                     <field name="batch" optional="show"/>
                </xpath>
            </field>
        </record>
        <record id="view_picking_internal_search2" model="ir.ui.view">
            <field name="name">stock.picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_internal_search"/>
            <field name="arch" type="xml">
                  <xpath expr="//field[@name='origin']" position="after">
                     <field name="batch"/>
                </xpath>
            </field>
        </record>
        <record id="workcenter_line_kanban_batch" model="ir.ui.view">
         <field name="name">mrp.workorder</field>
          <field name="model">mrp.workorder</field>
        <field name="inherit_id" ref="mrp.workcenter_line_kanban"/>

         <field name="arch" type="xml">

          <xpath expr="//field[@name='name']" position="after">
                     <field name="batch" />
          </xpath>
             <xpath expr="//div[1]//div[1]//div[1]" position="after">
                 <div class="o_kanban_record_headings o_kanban_card_header_title">
                                    <strong class="o_primary ml8">
                                        <span><t t-esc="record.batch.value"/></span>
                                    </strong>
                 </div>

          </xpath>


         </field>
        </record>

            <template id="stock_report_delivery_document_inherit_mrp_updates" inherit_id="stock.report_delivery_document">
            <xpath expr="//div[@class='row mt32 mb32']" position="after">
                <div class="row mt32 mb32"> </div>
                <div class="row mt32 mb32">

                <div t-if="o.batch" class="col-auto" name="batch">
                            <strong>Batch Number:</strong>
                            <p t-field="o.batch"/>
                        </div>
                <div t-if="o.mrp_product_id" class="col-auto" name="mrp_product_id">
                            <strong>Product:</strong>
                            <p t-field="o.mrp_product_id"/>
                </div>
                             </div>

            </xpath>
            </template>
<record id="stock_return_picking_inherit" model="ir.ui.view">
    <field name="name">stock.return.picking</field>
    <field name="model">stock.return.picking</field>
    <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
    <field name="arch" type="xml">

        <xpath expr="//field[@name='move_dest_exists']" position="after">
            <field name="production_id" invisible="1"/>
        </xpath>

    </field>
</record>


        <menuitem id="mrp.menu_mrp_root"
        name="Manufacturing"
        groups="mrp.group_mrp_user,mrp.group_mrp_manager,group_production_user"
        web_icon="mrp,static/description/icon.png"
        sequence="35"/>
    </data>

       <data noupdate="1">
    <record id="seq_item_request" model="ir.sequence">
        <field name="name">Picking Receipts From Production</field>
        <field name="code">production.picking</field>
        <field name="prefix">RP/%(y)s</field>
        <field name="padding">5</field>
    </record></data>
</odoo>