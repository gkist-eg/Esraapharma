<odoo>

    <record id="repot_edit" model="ir.ui.view">
          <field name="name">PriceListAccount</field>
          <field name="model">sale.report</field>
          <field name="inherit_id" ref="sale.view_order_product_search"/>
          <field name="arch" type="xml">

                          <search string="Sales Analysis">
                <field name="cust_categ_id" />

                <group expand="1" string="Group By">
                    <filter string="Customer Category" name="cust_categ_id" context="{'group_by':'cust_categ_id'}"/>

                </group>
            </search>


          </field>
      </record>
</odoo>