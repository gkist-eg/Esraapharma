<odoo>
        <record id="view_warehouse_form" model="ir.ui.view">
            <field name="name">multi.warehouse.form</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <group name="preferences" position="after">
                    <group>
                        <field name="multi_warehouse"  widget="many2many_tags" options="{'no_create_edit': True, 'no_create':True }"/>
                    </group>
                </group>
            </field>
        </record> <record id="view_warehouse_form_edit" model="ir.ui.view">
            <field name="name">sale.order.form.edit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale_stock.view_order_form_inherit_sale_stock"/>
            <field name="arch" type="xml">
                <field name="warehouse_id" position="replace" >
                    <field name="warehouse_id"  />
                </field>

            </field>
        </record>
    <record id="view_partner_form_partner_edit" model="ir.ui.view">
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">
                
                 <xpath expr="//field[@name='phone']" position="after">
                    <field name="limit_credit" />
                </xpath>
            </field>
        </record>

     <record id="view_warehouse" model="ir.ui.view">
        <field name="name">stock.warehouse</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">


          <xpath expr="//field[@name='partner_id']" position="before">
                    <field name="sale_store"/>
                </xpath>
        </field>
    </record>





</odoo>
