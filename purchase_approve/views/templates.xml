<odoo>
    <data>
<record id="group_mfo_user" model="res.groups">
    <field name="name">Manage Manufacturing For Other</field>
    <field name="category_id" ref="base.module_category_hidden"/>
</record>    </data>

<template id="report_purchaseorder_document" inherit_id="purchase.report_purchaseorder_document">
            <xpath expr="//h2[1]" position="replace">
                <br/>
            <h2 t-if="o.state not in ['purchase', 'done']">Request for Quotation #<span t-field="o.name"/></h2>
  <h2 t-if="o.state == 'draft'">Request for Quotation #<span t-field="o.name"/></h2>

            </xpath>
    <xpath expr="//h2[2]" position="replace">

            </xpath>

    <xpath expr="//div[@id='informations']" position="inside">
          <div t-if="o.payment_term_id" class="col-3 bm-2">
                    <strong>Payment Term :</strong>
                    <p t-field="o.payment_term_id" class="m-0"/>
                </div>
            </xpath>

    <xpath expr="//div[@id='total']" position="after">
          <div id="signiture" class="row mt32 mb32">
                <div class="col-3 bm-2">
                    <strong>Prepared by</strong>
                    <p class="m-0"/>
                </div>
                <div  class="col-3 bm-2">
                    <strong>Approved by</strong>
                    <p  class="m-0"/>
                </div>
                <div class="col-3 bm-2">
                    <strong>Authorized by</strong>
                    <p  class="m-0"/>
                </div>
              <div class="col-3 bm-2">
                    <strong>Supplier Approval</strong>
                    <p  class="m-0"/>
                </div>
              <br/>
              <br/>
              <br/>
            </div>



    </xpath>
        </template>

</odoo>